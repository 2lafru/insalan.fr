<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8"/>
    {% block stylesheets %}
      {% stylesheets 'bundles/insalan/css/global.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
      {% endstylesheets %}
    {% endblock %}

    <link href="{{ asset('bundles/insalan/img/favicon.png') }}" rel="icon" type="image/png"/>
    <title>{% block title %}InsaLan{% endblock %}</title>

    <!--[if lt IE 9]>
      <script src="{{ asset('bundles/insalan/js/html5shiv.js') }}"></script>
    <![endif]-->
  </head>

  <body id="body">
    <header>
      <div class="container">
        <div class="logo">
          <div class="pattern"></div>
          <a href="{{ path('insalan_news_default_index') }}">
            <img src="{{ asset('bundles/insalan/img/logo.png') }}" alt="InsaLan"/></a>
        </div>

        <aside>
          <span class="day">21</span>
          <span class="month">Février</span>
          <span class="place">Rennes</span>
        </aside>

        <nav>
          <ul>
            <li><a href="{{ path('insalan_news_default_index') }}">Actualités</a></li>
            <li><a href="{{ path('insalan_tournament_default_index') }}">Tournois</a></li>
            <li><a href="{{ path('insalan_pizza_default_index') }}">Pizzas</a></li>
            <li><a href="exhibition.html">Informations</a></li>
            <li><a href="lan.html">Galerie</a></li>
          </ul>
        </nav>
      </div>
    </header>

    <section class="container">
      {% for message in app.session.flashbag.get('info') %}
        <p class="alert-info">{{ message }}</p>
      {% endfor %}

      {% for message in app.session.flashbag.get('error') %}
        <p class="alert-error">{{ message }}</p>
      {% endfor %}

      {% block body %}{% endblock %}

      <div class="clear"></div>
    </section>

    <footer>
      <div>
        <aside>
          <div>
            <a id="logo" href="index.html"></a>
            <a id="facebook" href="https://www.facebook.com/onluxgaming"></a>
            <a id="twitter" href="https://twitter.com/OnLuxGaming"></a>
            <div id="mail"><a href="mailto:info@onlux-gaming.lu">info@onlux-gaming.lu</a></div>
          </div>
        </aside>
        <nav>
          <ul>
            <li id="about">
              <header><a href="about.html">About</a></header>
              <ul>
                <li><a href="about.html#project">Project</a></li>
                <li><a href="about.html#team">Team</a></li>
                <li><a href="about.html#event">Event</a></li>
                <li><a href="about.html#map">Access map</a></li>
              </ul>
            </li>
            <li id="ex">
              <header><a href="exhibition.html">Exhibition</a></header>
              <div>
                <ul>
                  <li><a href="exhibition.html#stands">Thematic stands</a></li>
                  <li><a href="exhibition.html#hours">Opening hours</a></li>
                  <li><a href="exhibition.html#schedule">Activities schedule</a></li>
                  <li><a href="exhibition.html#food">Meal &amp; Drink</a></li>
                </ul>
                <ul class="second">
                  <li id="stage"><a href="exhibition.html#stage">Main Stage</a></li>
                  <li><a href="exhibition.html#stageplan">Schedule</a></li>
                </ul>
              </div>
            </li>
            <li id="lan">
              <header><a href="lan.html">Lan</a></header>
              <div>
                <ul>
                  <li><a href="lan.html#games">Tournaments</a></li>
                  <li><a href="lan.html#hours">Opening hours</a></li>
                  <li><a href="lan.html#tickets">Tickets</a></li>
                  <li><a href="lan.html#accomodations">Accomodations</a></li>
                </ul>
                <ul class="second">
                  <li><a href="lan.html#rules">Event rules</a></li>
                  <li><a href="lan.html#cash">Cashprizes</a></li>
                  <li><a href="lan.html#sponsors">Sponsors</a></li>
                </ul>
              </div>
            </li>
            <li id="partner">
              <header><a href="partners.html">Partners</a></header>
              <ul>
                <li><a href="partners.html#admin">Administrative</a></li>
                <li><a href="partners.html#vg">Video Games</a></li>
                <li><a href="partners.html#asso">Associations</a></li>
                <li><a href="partners.html#food">Food &amp; Drink</a></li>
              </ul>
            </li>
          </ul>
        </nav>
        <div id="copyright">
          <p>
            © 2013 OnLux Gaming<span>|</span>Design by <a href="http://www.alecahain.fr/">Aceltis</a><span>|</span>Code by Aceltis, Chris, Jerry and Thierno
          </p>
        </div>
      </div>
    </footer>

    {% block javascripts %}
      {% javascripts
        'bundles/insalan/js/mootools-core-1.4.5.js'
        'bundles/insalan/js/mootools-more-1.4.0.1.js'
        'bundles/insalan/js/global.js' %}
          <script type="text/javascript" src="{{ asset_url }}"></script>
      {% endjavascripts %}
    {% endblock %}
  </body>

<!-- Mirrored from onlux-gaming.lu/ by HTTrack Website Copier/3.x [XR&CO'2013], Sun, 22 Dec 2013 11:30:36 GMT -->
</html>
